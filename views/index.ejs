    <section id="contact" style="background: url(&quot;assets/img/header-bg-7.jpg&quot;);">
        <div class="container" style="padding: 45px;">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="text-uppercase section-heading" style="text-shadow: -4px 0px 20px rgb(0,0,0);color: rgb(254,209,54);">Entre em contato conosco</h2>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <form id="contactForm" name="contactForm" novalidate="novalidate">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group mb-3"><input class="form-control" type="text" id="name" placeholder="Seu nome *" required="" style="background: rgb(52,58,64);border-color: rgb(254,209,54);"><small class="form-text text-danger flex-grow-1 help-block lead"></small></div>
                                <div class="form-group mb-3"><input class="form-control" type="email" id="email" placeholder="Seu email *" required="" style="background: rgb(52,58,64);border-color: rgb(254,209,54);"><small class="form-text text-danger help-block lead"></small></div>
                                <div class="form-group mb-3"><input class="form-control" type="tel" placeholder="Seu telefone *" required="" style="background: rgb(52,58,64);border-color: rgb(254,209,54);"><small class="form-text text-danger help-block lead"></small></div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group mb-3"><textarea class="form-control" id="message" placeholder="Sua mensagem *" required="" style="height: 230px;background: rgb(52,58,64);border-color: rgb(254,209,54);"></textarea><small class="form-text text-danger help-block lead"></small></div>
                            </div>
                            <div class="w-100"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div><button class="btn btn-primary btn-xl text-uppercase" id="sendMessageButton" type="submit" style="color: rgb(0,0,0);">enviar mensagem</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
